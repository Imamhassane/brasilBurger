    <!DOCTYPE html>
    <html>
        <head>
            <meta charset="UTF-8">
            <title>{% block title %}Welcome!{% endblock %}</title>

            
            <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
            
            {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
            
            <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
            
            <!-- font awesome cdn link  -->
                <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">

                <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css">

            <!-- custom css file link  -->
                <link rel="stylesheet" href="/stylesheets/screen.css">
            
            <!-- cdn link   -->
                <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg==" crossorigin="anonymous" referrerpolicy="no-referrer"/>
            
            <!-- select2 -->
                <link href="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/css/select2.min.css" rel="stylesheet" />

            {% block stylesheets %}
            {% endblock %}

            {% block javascripts %}
            {% endblock %}
        </head>
        <body>
            <header class="header">
                <div id="menu-btn" class="fas fa-bars icons"></div>
                <div id="search-btn" class="fas fa-search icons" style="visibility:hidden"></div> 
                <nav class="navbar">
                    {% block naavbarLinks %}
                        {% block Gestionnaire %}
                            <a href="{{path('dashboard')}}" id="link-dashoard" class="nav-links">Tableau de bord</a>
                            <a href="{{path('listMenu')}}" id="link-produitList" class="nav-links">Produits</a>
                            <a href="{{path('listCommande')}}" id="link-commandes" class="nav-links">Commandes</a>
                            <a href="{{path('archiveList')}}" id="link-archives" class="nav-links"> Archives </a>
                        {% endblock %}
                        {% block Client %}
                            <a href="{{path('catalogue')}}" id="link-catalogue" class="nav-links">Catalogue</a>
                            <a href="{{path('mescommandes')}}" id="link-mescommandes" class="nav-links">Commandes</a>
                            <a href="{{path('panier')}}" id="link-panier"  class="nav-links panierLink">
                                <i class="fa-solid fa-cart-shopping" id="svg"></i>                    
                                <span id="compteur" name="compteur" value=""></span>
                            </a>
                        {% endblock %}
                    {% endblock %}
                    {# <a href="#" class="fas fa-shopping-cart fa-3x"></a>  #}
                    {% block login %}
                        <div class="icons d-flex justify-content-end ml-auto mt-3">
                            {% block loginSignIn %}
                                <a href="{{path('panier')}}" id="link-paniernotconnected" id="showCommandeToken"  class="nav-links panierLink">
                                    <i class="fa-solid fa-cart-shopping top" id="svg"></i>                    
                                    <span id="compteur" name="compteur" value=""></span>
                                </a>
                                <a id="loginButton" href="{{path('login')}}" class="btn mr-md-4">Se connecter</a>
                                <a id="singinButton"href="{{path('newClient')}}" class="btn ">Créer un compte</a>
                            {% endblock %}
                            {% block logout %}
                                <a id="deconnexion" href="{{path('logout')}}" style="margin-top:2rem" class="btn mr-md-4">Deconnexion</a>
                            {% endblock %}
                        </div>
                    {% endblock %}
                </nav>
                <a href="{{path('catalogue')}}" class="logo"><img src="/images/logo.png" alt=""></a>
                {# <form action="" class="search-form">
                    <input type="search" name="" placeholder="search here..." id="search-box">
                    <label for="search-box" class="fas fa-search icons"></label>
                </form> #}
            </header>
            {% block body %}{% endblock %}

            <!-- AOS     -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"></script>
            <script> AOS.init({duration: 800,});</script>
            <script>
                function filterByDaye(e) {
                    document.location.replace("/commande+"+choiceDate.value)
                }
                const option = document.getElementById('option');
                option.addEventListener("change",(e)=>{
                    document.location.replace("/mescommande"+option.value)
                })
                
            </script>
            <!-- custom js file link      -->
            <script src="/js/script.js"></script>
            <script src="/js/reglerPaiement.js"></script>
            {# <script src="http://code.jquery.com/jquery-1.11.0.min.js"></script> #}
            <script src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js"></script>

        </body>
    </html>

